<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} [%t] %highlight{%-5p} %c:%L - %msg%n"/>
		</Console>
	</Appenders>

	<Loggers>
		<Root level="info">
			<Appender-Ref ref="STDOUT"/>
		</Root>
	</Loggers>
</Configuration>
